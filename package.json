  "devDependencies": {
    "eslint": "^1.5.0",
    "json-loader": "^0.5.3",
    "node-libs-browser": "^0.5.3",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0",
    "webpack": "^1.12.2"
  },
  "homepage": "https://github.com/ben-eb/cssnano",
  "author": {
